<div>
    <h2> Log In </h2>

    <%= form_tag '/login' do %>
        Username: <%= text_field_tag :username %> <br>
        Password: <%= password_field_tag :password %> <br>
        
        <%= submit_tag "Log In" %>
    <% end %>

    <p>Don't have an account? <%= link_to 'Sign up', signup_path %></p>
</div>
